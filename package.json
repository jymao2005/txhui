  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p [browserify-hmr -h 0.0.0.0 --url http://chip.jymao.com:9500 --port 9500]  --global-transform babelify --global-transform vueify --global-transform [ browserify-css --minify=true]  -e src/main.js -o dist/build.js",
    "serve": "http-server -p 9300 -o -c 1 -a 0.0.0.0",
    "dev": "npm-run-all --parallel watchify serve",
    "lint": "eslint --ext .js,.vue src test/unit",
    "test": "karma start karma.conf.js",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] --global-transform babelify --global-transform vueify --global-transform [ browserify-css --minify=true]  -e src/main.js | uglifyjs -c warnings=false -m | tee  dist/build.js | gzip >dist/build.js.gz"
  },
  "browserify": {
    "transform": []
  },
